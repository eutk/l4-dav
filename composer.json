{
    "name": "ngmy/l4-dav",
    "type": "library",
    "description": "A simple WebDAV client library for Laravel",
    "keywords": [
        "laravel",
        "webdav"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuta Nagamiya",
            "email": "y.nagamiya@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/psr7": "^1.7.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "league/uri": "^6.4.0",
        "php-http/curl-client": "^1.7.1"
    },
    "require-dev": {
        "mikey179/vfsstream": "1.6.8",
        "mockery/mockery": "1.4.2",
        "nunomaduro/larastan": "0.6.11",
        "orchestra/testbench": "4.13.0|5.12.1|6.7.2",
        "phpstan/phpstan-mockery": "0.12.11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ngmy\\L4Dav\\L4DavServiceProvider"
            ],
            "aliases": {
                "L4Dav": "Ngmy\\L4Dav\\L4DavFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Ngmy\\L4Dav\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ngmy\\L4Dav\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "@php tools/phpunit --colors=always"
        ],
        "lint": [
            "@php tools/phpcs --colors"
        ],
        "fixlint": [
            "@php tools/phpcbf"
        ],
        "findbugs": [
            "@php tools/phpstan analyse --ansi"
        ]
    }
}
